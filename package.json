{
  "name": "Angular_Starter_ES6",
  "version": "0.0.1",
  "description": "Starter for Angular ES6  Wepback",
  "main": "index.js",
  "scripts": {
    "start": "node_modules/.bin/gulp serve",
    "build": "node_modules/.bin/gulp webpack",
